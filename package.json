{
  "license": "MIT",
  "version": "1.0.0",
  "author": "Alask-Code",
  "main": "elec/main.js",
  "name": "altered-escape-v2",
  "description": "Alone Escape Launcher for EFT Mod.",
  "keywords": [
    "Escape from Tarkov",
    "Altered Escape",
    "Alone Escape",
    "EFT"
  ],
  "scripts": {
    "dev": "nodemon --exec electron .",
    "test": "electron .",
    "build": "electron-builder -p=never",
    "publish": "electron-builder -p=always"
  },
  "build": {
    "productName": "AE Launcher",
    "appId": "com.alaskcode.aelauncher",
    "nsis": {
      "publish": {
        "provider": "github",
        "owner": "Alask-code",
        "repo": "AE-Launcher-v2"
      },
      "oneClick": false
    },
    "devDependencies": {
      "@electron-internal/eslint-config": "^1.0.1",
      "electron": "^33.2.0",
      "electron-builder": "^25.1.8",
      "electron-reload": "^2.0.0-alpha.1",
      "eslint": "^8.57.1",
      "eslint-plugin-node": "^11.1.0",
      "nodemon": "^3.1.7"
    },
    "dependencies": {
      "dotenv": "^16.4.5",
      "uuid": "^11.0.3"
    }
  }
}